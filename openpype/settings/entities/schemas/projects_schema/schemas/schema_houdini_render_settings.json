{
    "type": "dict",
    "collapsible": true,
    "key": "RenderSettings",
    "label": "Render Settings",
    "children": [
        {
            "type": "text",
            "key": "default_render_image_folder",
            "label": "Default render image folder"
        },
        {
            "key": "aov_separator",
            "label": "AOV Separator character",
            "type": "enum",
            "multiselection": false,
            "default": "underscore",
            "enum_items": [
                {"dash":  "- (dash)"},
                {"underscore":  "_ (underscore)"},
                {"dot": ". (dot)"}
            ]
        },
        {
            "key": "image_prefix",
            "label": "Image prefix template",
            "type": "text"
        },
        {
            "key": "image_format",
            "label": "Output Image Format",
            "type": "enum",
            "multiselection": false,
            "defaults": "exr",
            "enum_items": [
                {"jpeg": "jpeg"},
                {"png": "png"},
                {"tif": "tif"},
                {"exr": "exr"}
            ]
        }
    ]
}
